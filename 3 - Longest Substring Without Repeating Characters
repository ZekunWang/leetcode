class Solution {
public:
    int lengthOfLongestSubstring(string s) {
        string s1 = "";
        int count = 0;
        for(int i = 0; i < s.length(); i++){
            for(int j = 0; j < s1.length(); j++){
                if(s[i] == s1[j]){
					if(s1.length() > count) count = s1.length();
                    s1.erase(0, j+1);
                    break;
                }
            }
            s1 += s[i];
        }
        return count > s1.length()? count:s1.length();
    }
};
